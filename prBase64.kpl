<?xml version="1.0" encoding="UTF-8"?><pag xmlns="www.kepler.com.mx/2003/KSchema" roles="" tit="Hola">	<grp dim="1,-1,10,20" brdr="2,1" clr="0,0,0" bclr="213,210,210"><br/><tab n="3"/>		<bot id="bot1" dim="10,0" brdr="5" text="Encode" OnEnter="Encode()"/><tab n="2"/>		<bot id="bot2" dim="10,0" brdr="5" text="Decode" OnEnter="Decode()"/>		<br/><br/><tab/><txt>String</txt><tab/>		<inp id="str" dim="30,1" brdr="2,1" bclr="255,255,255" type="string"/><br/><tab/><txt>Base 64</txt><tab/>		<inp id="base64" dim="30,1" brdr="2,1" bclr="255,255,255" type="string"/>		<br/><br/><br/><tab n="3"/>		<bot id="bot3" dim="10,0" brdr="5" text="EncodeFile" OnEnter="EncodeFile()"/><tab n="2"/>		<bot id="bot4" dim="10,0" brdr="5" text="DecodeFile" OnEnter="DecodeFile()"/>		<br/><br/><tab/><txt>Archivo</txt><tab/>		<inp id="file" dim="30,1" brdr="2,1" bclr="255,255,255" type="string"/><br/><tab/><txt>Base 64</txt><tab/>		<inp id="base64File" dim="30,1" brdr="2,1" bclr="255,255,255" type="string"/>		<br/>	</grp>	<script>		InstallDriver("KDB75");		class Main {			var frn,kdb;			fun Main() {				frn=new FRNT;				kdb=new KDB;				kdb.connect();				frn.set("str","Hola como estas?");				frn.set("file","SATmx/arch.pdf");				//kdb.sat("base64","datResp",0,"cadena");//tipo 0=codifica la cadena dada a base64				//kdb.sat("base64","datResp",2,"SATmx/Keys/certificado.pfx");//tipo 2=especifica un archivo en el servidor del cual se sacara el base 64				//kdb.sat("base64","datResp",10,"cadenaBase64");//tipo 10=decodifica un string base 64, aunque es mas para prueba, puesto que la cadena devuelta, podria contener caracteres no imprimibles y kepler no lo soportaria;				//kdb.sat("base64","datResp",12,"cadenaBase64","SATmx/Keys/certificado.pfx");//tipo 12=decodifica un string base 64, y lo guarda como un archivo en servidor sucbarpeta "xsd"			}			fun Encode(){				kdb.rst("datBase64");				kdb.sat("base64","datBase64",0,frn.get("str"));				kdb.exec();				frn.set("base64",kdb.get("datBase64","base64"));			}			fun Decode(){				kdb.rst("datStr");				kdb.sat("base64","datStr",10,frn.get("base64"));				kdb.exec();				frn.set("str",kdb.get("datStr","string"));			}			fun EncodeFile(){				kdb.rst("datBase64");				kdb.sat("base64","datBase64",2,frn.get("file"));				kdb.exec();				frn.set("base64File",kdb.get("datBase64","base64"));			}			fun DecodeFile(){				kdb.rst("datStr");				kdb.sat("base64","datFile",12,frn.get("base64File"),frn.get("file")&"b64decoded");				kdb.exec();			}		}	</script></pag>